  "license": "MIT",
  "scripts": {
    "start": "pnpm dev",
    "dev": "concurrently -n web,server,yjs -c green,cyan,magenta \"pnpm web:dev\" \"pnpm dev:server\" \"pnpm dev:yjs\"",
    "dev:web": "pnpm web:dev",
    "dev:server": "pnpm nx serve server",
    "dev:yjs": "pnpm yjs",
    "web:dev": "vite --config apps/web/vite.config.ts",
    "web:build": "vite build --config apps/web/vite.config.ts",
    "web:preview": "vite preview --config apps/web/vite.config.ts",
    "server": "pnpm nx serve server",
    "build": "pnpm build:web && pnpm build:server",
    "build:web": "pnpm nx build web || pnpm web:build",
    "build:server": "pnpm nx build server",
    "start:prod": "pnpm build && concurrently -n api,web,yjs -c cyan,green,magenta \"node dist/apps/server/main.js\" \"pnpm web:preview\" \"pnpm yjs\"",
    "db:generate": "prisma generate --schema=./packages/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./packages/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./packages/database/prisma/schema.prisma",
    "yjs": "node apps/server/src/y-websocket-start.js"
  },
  "private": true,
